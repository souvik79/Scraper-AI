# ScraperAPI (required)
SCRAPER_API_KEY=your_scraper_api_key_here

# AI Providers (at least one required)
OPENAI_API_KEY=your_openai_key_here
ANTHROPIC_API_KEY=your_anthropic_key_here
CLAUDE_MODEL=claude-haiku-4-5-20251001

# Ollama (local, no key needed)
OLLAMA_BASE_URL=http://localhost:11434
OLLAMA_MODEL=phi4-mini

# Groq (free tier — OpenAI-compatible)
GROQ_API_KEY=your_groq_key_here
GROQ_MODEL=llama-3.1-8b-instant

# Gemini (free tier — generous limits, large context)
# Get a key at https://aistudio.google.com/apikey
GEMINI_API_KEY=your_gemini_key_here
GEMINI_MODEL=gemini-2.5-flash

# Default provider for Phase 3 extraction: anthropic | openai | ollama | groq | gemini
DEFAULT_PROVIDER=anthropic

# Phase 2 processor (optional — dual-model mode)
# Set to a provider name for HTML understanding before extraction
# Leave empty for single-model mode (provider handles everything)
# PROCESSOR_PROVIDER=gemini
